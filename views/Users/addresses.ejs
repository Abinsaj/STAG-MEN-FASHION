                                                    <address>  
                                                        <%= element.name %><br>                                     
                                                        <%= element.mobile %><br>
                                                        <%= element.locality %><br>
                                                        <%= element.Address %><br>
                                                        <%= element.city %><br>
                                                        <%= element.state %><br>
                                                        <%= element.country %>
                                                    </address>
                                                 
                                                
