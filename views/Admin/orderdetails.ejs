    <script>
        function changestatus(orderID, currentStatus) {
            console.log(orderID);
            const selectedStatus = document.getElementById('select').value;
            console.log(selectedStatus);
            if (selectedStatus !== currentStatus) {
                Swal.fire({
                    title: 'Are you sure?',
                    text: 'Are you sure you want to change the status!',
                    icon: 'warning',
                    showCancelButton: true,
                    confirmButtonText: 'Yes, change it!',
                    cancelButtonText: 'No, cancel!',
                }).then((result) => {
                    if (result.isConfirmed) {
                        console.log("its hereeeeeeeee");
                        fetch('orderdetails', {
                            method: 'post',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                orderID: orderID,
                                statusID: selectedStatus
                            })
                        })
                        .then(response => response.json())
                        .then(data => {
                            if (data.success) {
                                Swal.fire({
                                    title: 'Status Changed!',
                                    text: 'The status has been successfully changed.',
                                    icon: 'success'
                                }).then(() => {
                                    // Additional logic or page refresh if needed
                                    window.location.reload();
                                });
                            } else {
                                Swal.fire({
                                    title: 'Error',
                                    text: 'Failed to change the status.',
                                    icon: 'error'
                                });
                            }
                        })
                        .catch(error => {
                            console.error('Error:', error);
                            Swal.fire({
                                title: 'Error',
                                text: 'An unexpected error occurred.',
                                icon: 'error'
                            });
                        });
                    }
                });
            } else {
                // Status not changed
                Swal.fire({
                    title: 'Info',
                    text: 'The status is already set to ' + currentStatus + '. No changes made.',
                    icon: 'info'
                });
            }
        }

        function back(){
            window.location.href = 'orderlist'
        }

    </script>
    

