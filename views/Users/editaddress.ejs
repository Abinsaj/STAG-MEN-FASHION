    <script>
        const nameid = document.getElementById('typeName')
        const mobileid = document.getElementById('typeMob')
        const localityid = document.getElementById('typeLoc')
        const pincodeid = document.getElementById('typePin')
        const addressid = document.getElementById('typeAdd')
        const cityid = document.getElementById('typeCity')
        const stateid = document.getElementById('typeState')
        const countryid = document.getElementById('typeCountry')
    
        const error1 = document.getElementById('error1')
        const error2 = document.getElementById('error2')
        const error3 = document.getElementById('error3')
        const error4 = document.getElementById('error4')
        const error5 = document.getElementById('error5')
        const error6 = document.getElementById('error6')
        const error7 = document.getElementById('error7')
        const error8 = document.getElementById('error8')
    
        const addform = document.getElementById('addform')
    
        function namevalidate(e){
            const nameval = nameid.value.trim();
        const namePattern = /^[A-Z][a-zA-Z]*(\s[A-Z][a-zA-Z]*)?$/
        if (nameval === "") {
            error1.style.display = "block"
            error1.innerHTML = "Please Enter a valid name."
        } else if (!namePattern.test(nameval)) {
            error1.style.display = "block"
            error1.innerHTML = "Invalid format for the Name. First letter should be capital."
        } else {
            error1.style.display = "none"
            error1.innerHTML = ""
        }
        }
    
        function mobvalidate(e){
        const mobval = mobileid.value.trim();
        const mobpattern = /^\d{10}$/
        if (mobval === "") {
           error2.style.display = "block";
           error2.innerHTML = "Please enter a Mobile Number.";
        } else if (!mobpattern.test(mobval)||/^(.)\1{9}$/.test(mobval)) {
           error2.style.display = "block";
           error2.innerHTML = "Please enter a valid 10-digit Mobile Number.";
        } else {
           error2.style.display = "none";
           error2.innerHTML = "";
        }
    
        }
      
        function locvalidate(e){
            const locval = localityid.value.trim()
            if (locval=="") {
                error3.style.display = "block"
                error3.innerHTML = 'locality cannot be empty'
            } else {
                error3.style.display = 'none';
                error3.innerHTML = ''
            }
        }
    
        function pinvalidate(e){
            const pinval = pincodeid.value.trim();
            const pinpattern = /^\d{6}$/
            if (pinval === "") {
                error4.style.display = 'block';
                error4.innerHTML = 'Please enter a pincode.';
            } else if (!pinpattern.test(pinval)|| /^(.)\1{5}$/.test(pinval)) {
                error4.style.display = 'block';
                error4.innerHTML = 'Pincode must be 6 digit';
            } else {
                error4.style.display = 'none';
                error4.innerHTML = '';
            }
        }
    
        function valAddress(e){
            const addinput = addressid.value.trim()
            if(addinput==""||addinput.length<8){
                error5.style.display = "block"
                error5.innerHTML = 'Please enter a valid address '
            }else{
                error5.style.display = 'none';
                error5.innerHTML = ''
            }
        }
    
        function cityvalidate(e){
            const cityVal = cityid.value.trim();
            if (cityVal === "") {
                error6.style.display = 'block';
                error6.innerHTML = 'Please enter a valid city name.';
            } else if (!/^[a-zA-Z]+$/.test(cityVal)) {
                error6.style.display = 'block';
                error6.innerHTML = 'City name should only contain letters.';
            } else {
                error6.style.display = 'none';
                error6.innerHTML = '';
            }
        }
    
        function statevalidate(e){
            const stateVal = stateid.value.trim();
            if (stateVal === "") {
                error7.style.display = 'block';
                error7.innerHTML = 'Please enter a valid city name.';
            } else if (!/^[a-zA-Z]+$/.test(cityVal)) {
                error7.style.display = 'block';
                error7.innerHTML = 'City name should only contain letters.';
            } else {
                error7.style.display = 'none';
                error7.innerHTML = '';
            }
        }
    
        function cntryvalidate(e){
            const cntryVal = countryid.value.trim();
            if (cntryVal === "") {
                error8.style.display = 'block';
                error8.innerHTML = 'Please enter a valid city name.';
            } else if (!/^[a-zA-Z]+$/.test(cityVal)) {
                error8.style.display = 'block';
                error8.innerHTML = 'City name should only contain letters.';
            } else {
                error8.style.display = 'none';
                error8.innerHTML = '';
            }
        }
    
        nameid.addEventListener('blur',namevalidate)
        mobileid.addEventListener('blur',mobvalidate)
        localityid.addEventListener('blur',locvalidate)
        pincodeid.addEventListener('blur',pinvalidate)
        addressid.addEventListener('blur',valAddress)
        cityid.addEventListener('blur',cityvalidate)
        stateid.addEventListener('blur',statevalidate)
        countryid.addEventListener('blur',cntryvalidate)
    
        addform.addEventListener('submit',function(e){
            namevalidate(),
            mobvalidate(),
            locvalidate(),
            pinvalidate(),
            valAddress(),
            cityvalidate(),
            statevalidate(),
            cntryvalidate()
    
            if(error1.innerHTML||error2.innerHTML||error3.innerHTML||error4.innerHTML||error5.innerHTML||error6.innerHTML||error7.innerHTML||error7.innerHTML||error8.innerHTML){
                e.preventDefault()
            }
        })
    
